{
  "name": "jfgi",
  "version": "0.9.9",
  "packageManager": "pnpm@9.14.4",
  "browserslist": [
    "> 0.5%",
    "last 10 versions",
    "node > 22",
    "not dead"
  ],
  "scripts": {
    "dev": "vite dev --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "emulate": "firebase emulators:start",
    "supported_browsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > src/lib/utils/supportedBrowsers.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.8.0",
    "@playwright/test": "1.49.0",
    "@sveltejs/enhanced-img": "0.4.1",
    "@sveltejs/kit": "2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/container-queries": "0.1.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "18.19.42",
    "@vitejs/plugin-basic-ssl": "^1.2.0",
    "autoprefixer": "10.4.20",
    "browserslist-useragent-regexp": "^4.1.3",
    "daisyui": "4.12.14",
    "eslint": "9.8.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-tailwindcss": "3.17.5",
    "firebase-tools": "13.28.0",
    "globals": "15.8.0",
    "postcss": "8.4.49",
    "prettier": "3.4.2",
    "prettier-plugin-svelte": "3.3.2",
    "prettier-plugin-tailwindcss": "0.6.5",
    "svelte": "5.5.2",
    "svelte-check": "^4.1.1",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "3.4.7",
    "tslib": "2.6.3",
    "typescript": "5.5.4",
    "typescript-eslint": "8.0.0-alpha.52",
    "vercel": "39.1.2",
    "vite": "6.0.2",
    "vitest": "2.1.8"
  },
  "type": "module",
  "dependencies": {
    "@sentry/tracing": "^7.114.0",
    "@sveltejs/adapter-vercel": "^5.5.0",
    "firebase": "^10.14.1"
  }
}
